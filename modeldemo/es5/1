
#include <iostream>
using namespace std;
class Clock{
//时钟类定义
public:
//外部接口
    clock(int nour-o,int minute=0,int second= 0);
    void showTime() const;
    clock& operator++();
//前置单目运算符重载变量
    clock operator++ (int);
//后置单目运算符重载
private:
//私有数据成员
    int hour,minute,second;
};
Clock::Clock(int hour/*=0 */,int minute/* =0 */,int second/*=0 */)